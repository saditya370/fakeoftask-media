<%dragActions = "dragstart->drag#dragStart 
                dragover->drag#dragOver 
                dragenter->drag#dragEnter
                drop->drag#drop
                dragend->drag#dragEnd" %>



<%=content_tag(:div, class: 'bi bi -grip-vertical draggable',style: 'margin-right:15px' ,draggable: true,data: {
controller: 'drag',
action: dragActions,
resource_id: post.id,
url: "/drag/post",
parent: 'posts',

}) do %>
<%= render post%>
<%end%>